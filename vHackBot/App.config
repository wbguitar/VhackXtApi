      </setting>
      <setting name="maxAntivirus" serializeAs="String">
        <value>15000</value>
      </setting>
      <setting name="hackDevPolling" serializeAs="String">
        <value>00:01:00</value>
      </setting>
      <setting name="hackBotnetPolling" serializeAs="String">
        <value>00:01:00</value>
      </setting>
